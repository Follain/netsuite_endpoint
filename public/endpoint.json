{
  "name": "netsuite",
  "display": "NetSuite Endpoint",
  "description": "Sync products, stock, customers, orders and shipments with NetSuite ERP",
  "help": "http://guides.spreecommerce.com/integration/endpoints/netsuite",
  "icon_url": "spree.png",
  "services": [
    {
      "name": "products",
      "path": "/products",
      "description": "Fetch products via NetSuite",
      "requires": {
        "parameters": [{
          "name": "api_version",
          "description": "NetSuite api version",
          "data_type": "string"
        }, {
          "name": "wsdl_url",
          "description": "NetSuite API url",
          "data_type": "string"
        }, {
          "name": "email",
          "description": "NetSuite user email",
          "data_type": "string"
        }, {
          "name": "password",
          "description": "NetSuite user password",
          "data_type": "password"
        }, {
          "name": "account",
          "description": "NetSuite account",
          "data_type": "string"
        }, {
          "name": "role_id",
          "description": "NetSuite role id",
          "data_type": "string"
        }, {
          "name": "sandbox",
          "description": "NetSuite sandbox",
          "data_type": "boolean"
        }, {
          "name": "last_updated_after",
          "description": "Date of last item imported",
          "data_type": "string"
        }]
      },
      "recommends": {
        "messages": ["netsuite:product:poll"],
        "schedule": {
          "count": 1,
          "interval": "minutes"
        }
      }
    },
    {
      "name": "inventory_stock",
      "path": "/inventory_stock",
      "description": "",
      "requires": {
        "parameters": [{
          "name": "api_version",
          "description": "NetSuite api version",
          "data_type": "string"
        }, {
          "name": "wsdl_url",
          "description": "NetSuite API url",
          "data_type": "string"
        }, {
          "name": "email",
          "description": "NetSuite user email",
          "data_type": "string"
        }, {
          "name": "password",
          "description": "NetSuite user password",
          "data_type": "password"
        }, {
          "name": "account",
          "description": "NetSuite account",
          "data_type": "string"
        }]
      },
      "recommends": {
        "messages": ["stock:query"]
      }
    },
    {
      "name": "orders",
      "path": "/orders",
      "description": "Exports orders to NetSuite",
      "requires": {
        "parameters": [{
          "name": "api_version",
          "description": "NetSuite API version",
          "data_type": "string"
        }, {
          "name": "wsdl_url",
          "description": "NetSuite API url",
          "data_type": "string"
        }, {
          "name": "email",
          "description": "NetSuite user email",
          "data_type": "string"
        }, {
          "name": "password",
          "description": "NetSuite user password",
          "data_type": "password"
        }, {
          "name": "account",
          "description": "NetSuite account",
          "data_type": "string"
        }, {
          "name": "role_id",
          "description": "NetSuite role id",
          "data_type": "string"
        }, {
          "name": "sandbox",
          "description": "NetSuite sandbox",
          "data_type": "boolean"
        }]
      },
      "recommends": {
        "messages": ["order:new"]
      }
    }, {
      "name": "shipments",
      "path": "/shipments",
      "description": "Fulfills orders on NetSuite",
      "requires": {
        "parameters": [{
          "name": "api_version",
          "description": "NetSuite api version",
          "data_type": "string"
        }, {
          "name": "wsdl_url",
          "description": "NetSuite API url",
          "data_type": "string"
        }, {
          "name": "email",
          "description": "NetSuite user email",
          "data_type": "string"
        }, {
          "name": "password",
          "description": "NetSuite user password",
          "data_type": "password"
        }, {
          "name": "account",
          "description": "NetSuite account",
          "data_type": "string"
        }, {
          "name": "role_id",
          "description": "NetSuite role id",
          "data_type": "string"
        }, {
          "name": "sandbox",
          "description": "NetSuite sandbox",
          "data_type": "boolean"
        }, {
          "name": "last_updated_after",
          "description": "Date of last item imported",
          "data_type": "string"
        }]
      },
      "recommends": {
        "messages": ["shipment:confirm"]
      }
    }
  ]
}
